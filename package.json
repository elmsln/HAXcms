{
  "name": "haxcms",
  "version": "2.0.9",
  "description": "CMS backend and rigging for a full on CMS",
  "main": "../index.html",
  "repository": {
    "type": "git",
    "url": "git@github.com/haxtheweb/haxcms.git"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@haxtheweb/baseline-build-hax": "^7.0.5",
    "@haxtheweb/elmsln-apps": "^7.0.8",
    "@haxtheweb/haxcms-elements": "^7.0.9"
  },
  "devDependencies": {
    "@open-wc/building-utils": "2.14.3",
    "@prettier/plugin-php": "0.14.0",
    "glob": "10.2.7",
    "gulp": "4.0.2",
    "gulp-minify-html-literals": "1.1.7",
    "gulp-terser": "2.1.0",
    "patch-package": "6.4.7",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "prettier": "1.16.4"
  },
  "scripts": {
    "lint": "prettier --check 'system/**/*.php' '*.{html,js,php,yml}'",
    "format": "prettier --write 'system/**/*.php' '*.{html,js,php,yml}'",
    "build": "ddev rm --all && polymer build && yarn run terser && yarn run wc-autoloader && yarn run version-match && mkdir build/es6/node_modules/@lrnwebcomponents && cp -R build/es6/node_modules/@haxtheweb/wc-autoload build/es6/node_modules/@lrnwebcomponents/wc-autoload && cp -R build/es6/node_modules/@haxtheweb/dynamic-import-registry build/es6/node_modules/@lrnwebcomponents/dynamic-import-registry && ddev start",
    "postinstall": "./node_modules/.bin/patch-package",
    "terser": "gulp terser --gulpfile=gulpfile.cjs",
    "wc-autoloader": "gulp wc-autoloader --gulpfile=gulpfile.cjs",
    "version-match": "gulp version-match --gulpfile=gulpfile.cjs",
    "start": "yarn run dev",
    "dev": "polymer serve --open --entrypoint dev.html --allow-origin --open-path=/",
    "nodeapp": "cd system/backend/nodejs && code . && yarn start"
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
